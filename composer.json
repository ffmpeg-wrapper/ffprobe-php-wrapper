{
  "name": "ffmpeg-wrapper/ffprobe-php-wrapper",
  "description": "A PHP wrapper around the mighty FFProbe.",
  "license": "MIT",
  "require": {
    "symfony/process": "^3.1",
    "symfony/property-access": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "FFMPEGWrapper\\FFProbe": "src/"
    },
    "files": [
      "src/Common.php"
    ]
  },
  "scripts": {
    "test": [
      "php vendor/bin/phpunit --configuration test/phpunit.xml test/cases/."
    ]
  }
}
